[user]
	name = <%= $git_name %>
	email = <%= $git_email %>

[alias]
	co = checkout
	ci = commit

[remote "ntt"]
	receivepack = /usr/local/bin/git-receive-pack
	uploadpack = /usr/local/bin/git-upload-pack

[core]
	quotepath = false
	excludesfile = ~/.gitignore

[color]
	diff = auto
	status = auto
	branch = auto

[github]
	user = <%= $github_username %>
	token = <%= $github_api_token %>

